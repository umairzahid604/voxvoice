{
    "name": "voxvoice",
    "version": "1.0.0",
    "description": "Client library for Voice Generation API",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "voice",
        "tts",
        "text-to-speech",
        "audio",
        "generation"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@ffmpeg-installer/ffmpeg": "^1.1.0",
        "@ffmpeg/ffmpeg": "^0.12.10",
        "fluent-ffmpeg": "^2.1.3"
    },
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    }
}